{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/01.microservice-proyect/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/01.microservice-proyect/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/01.microservice-proyect",
  "projects": {
    "01.microservice-proyect": {
      "type": "application",
      "root": "apps/01.microservice-proyect",
      "entryFile": "main",
      "sourceRoot": "apps/01.microservice-proyect/src",
      "compilerOptions": {
        "tsConfigPath": "apps/01.microservice-proyect/tsconfig.app.json"
      }
    },
    "mailapp": {
      "type": "application",
      "root": "apps/mailapp",
      "entryFile": "main",
      "sourceRoot": "apps/mailapp/src",
      "compilerOptions": {
        "tsConfigPath": "apps/mailapp/tsconfig.app.json"
      }
    }
  }
}